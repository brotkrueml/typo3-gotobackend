{
	"name": "brotkrueml/typo3-gotobackend",
	"type": "typo3-cms-extension",
	"description": "Add a button in Admin Panel to jump from a frontend page into the according backend area",
	"keywords": [
		"TYPO3 CMS",
		"TYPO3 extension"
	],
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Chris MÃ¼ller",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.2",
		"typo3/cms-adminpanel": "^10.4 || ^11.3",
		"typo3/cms-core": "^10.4 || ^11.3"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.15",
		"friendsofphp/php-cs-fixer": "^2.19",
		"typo3/coding-standards": "^0.3"
	},
	"config": {
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		},
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"extension-key": "gotobackend",
			"web-dir": ".Build/web"
		}
	},
	"autoload": {
		"psr-4": {
			"Brotkrueml\\GoToBackend\\": "Classes"
		}
	},
	"support": {
		"issues": "https://github.com/brotkrueml/typo3-gotobackend/issues",
		"source": "https://github.com/brotkrueml/typo3-gotobackend",
		"docs": "https://docs.typo3.org/p/brotkrueml/typo3-gotobackend/master/en-us/"
	}
}
